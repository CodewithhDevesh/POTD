#include<iostream>
#include<string>
using namespace std;
int main(){
    string s = "codeforces";
    string t;
    int count = 0;
    for(int i = 0 ; i < s.size() ; i++){
        cin>>t[i];
    }
    for(int i = 0 ; i < s.size() ; i++){
      if(s[i] != t[i])
      count++;
    }
    cout<<count;
}
//time complexity O(n);
//space complexity O(n);
